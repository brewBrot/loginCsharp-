        </Grid.RenderTransform>
        <Label Content="rEGisTEr" HorizontalAlignment="Left" Height="46" Margin="164,0,0,0" VerticalAlignment="Top" Width="118" FontFamily="Comic Sans MS" FontSize="24" FontWeight="Bold"/>
